<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mydemo.demo.dao.SysUserDao">

    <select id="selectAll" resultType="com.mydemo.demo.domain.SysUser">
        select * from sys_user;
    </select>

    <insert id="addUser"  parameterType="com.mydemo.demo.domain.SysUser">
            insert into sys_user(id,user_name,password,account)
            values
            <foreach collection="asd" item="item"  separator="," >
            (#{item.id},#{item.userName},"456",#{item.account})
            </foreach>
    </insert>

	
</mapper>